# Database Configuration
DATABASE_URL=postgresql://username:password@localhost:5432/agroph_db

# JWT Secret
JWT_SECRET=your_super_secret_jwt_key_here

# Server Configuration
PORT=3000
NODE_ENV=production

# Weather API (OpenWeatherMap)
